/**
 * @description  분리 된 메뉴, binddataset, formats, resize 변경
 * @param objGrd  - Grid
 * @param arr  	  - array
 * @return       N/A
 */
this.fn_popupMenuSet = function(objGrd, arr)
{
	var grdHeight = (26 * arr[2]) + 30;
	var grdWidth = (180 * 6);
	//trace(arr[3]);
	objGrd.set_binddataset( arr[1] );
	objGrd.set_formats( arr[0] );
	objGrd.set_height( grdHeight );
	
	
	var popupDivHeight = 37 + grdHeight + 5;
	var popupDivWidth = grdWidth;
	
	objGrd.set_width( grdWidth );
	objGrd.parent.set_height( popupDivHeight );
	objGrd.parent.set_width( popupDivWidth );
	//objGrd.parent.set_scrollbars("none");
}

this.fn_popupMenuStaticSet = function(objPdv, arr)
{
	var sum = 0;
	var height = 26;
	var menuBtnheight = 37;
	var fixWidth = 180;		// static width
	var objStatic;
	var satId;

	for(var i = 0; i < arr.length; i++)
		sum = sum + arr[i]+1;

	if(arr.length == 1)
	{
		if(arr[0] == 0)
			sum = 3;
	}

	sum = (sum * height) + (10*arr.length);
	
	
	objStatic = new Static("sta_bg1", "absolute", 0, menuBtnheight, 1080, sum, null, null);
	objPdv.addChild("sta_bg1", objStatic);
	objStatic.set_cssclass("sta_TF_subMenubg");
	objStatic.show();
	objStatic.sendToBack();	
	
	for(var i = 1; i < 6; i++)	// 최대 5개만 표현
	{
		satId = "sta_bar0" + i;
		objStatic = new Static(satId, "absolute", fixWidth*i, menuBtnheight+1, 1, sum-2, null, null);
		objPdv.addChild(satId, objStatic);
		objStatic.style.set_background("#e5e5e5ff");
		objStatic.show();
		objStatic.bringToFront();
	}
	
	objPdv.set_height(sum+menuBtnheight+5);
}

/**
 * @description  전체 메뉴 레벨 별로 분리
 * @param objGrd  - Grid
 * @return       N/A
 */
this.fn_descMenuSeparate = function(objDs)
{
	var nDs = new Dataset;
	this.addChild("nDs", nDs);
	nDs.copyData(objDs);
	
	var newDs = new Dataset;
	this.addChild(nDs.name+nDs.name, newDs);
	
	nDs.set_enableevent(false);
	nDs.filter( "CD_LEV == '2'");
	var cnt1 = nDs.rowcount;
	var formats;
	
	//var formats = this.fn_gridMenuForamtsCreate(nDs);	// grid format 생성 grid bind dataset
	
	for(var i = 1; i <= cnt1; i++)	// 데이터셋 컬럼 생성
	{
		newDs.addColumn( "column0"+(i-1), "STRING" );
	}
	var nRow = 0;
	nRow = newDs.addRow();
	newDs.setColumn(0, "column00", nDs.getColumn(0, "MENU_NM"));
	
	nDs.filter( "" );
	nDs.set_enableevent(true);
	
	var fixlev = nDs.getColumn(1, "CD_LEV");
	cnt1 = nDs.rowcount;
	var cnt2 = newDs.getColCount();
	var lev;
	var col = 0;
	var MENU_NM;
	var preRow = 1;
	
	for(var j = 2; j < cnt1; j++)	// 메뉴 분리
	{
		lev = nDs.getColumn(j, "CD_LEV");
		
		if(fixlev == lev)
		{
			col = col+1;
			MENU_NM = nDs.getColumn(j, "MENU_NM")
			newDs.setColumn(0, "column0"+col, MENU_NM);
			nRow = 1;
		}
		else
		{
			//trace("1) "+"newDs.rowcount: "+ (newDs.rowcount) + " preRow: " + preRow +" nRow: "+ nRow + "   " + nDs.getColumn(j, "MENU_NM"));
			if(newDs.rowcount == nRow)
			{
				nRow = newDs.addRow();
			}
			
			MENU_NM = nDs.getColumn(j, "MENU_NM");
			newDs.setColumn(nRow, "column0"+col, MENU_NM);
			nRow++;
		}
	}
	
	var nDsRemove = this.removeChild("nDs");
	nDsRemove.destroy();
	nDsRemove = null;
	
	return [formats, newDs, objDs];
}


