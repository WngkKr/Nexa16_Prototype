this.fn_fn_descMenuSeparateStatic = function(objPdv, objArr)
{
 	var nDs = objArr[1];
 	objPdv.menudataset = objArr[2];		//origin dataset
 	var colCnt =  nDs.colcount;
 	var rowCnt =  nDs.rowcount;
 	var objStatic;	// static object
 	var staId;		// static id
 	var sta = 0;	// static id naming count
 	var colId;		// column id
 	var staText;	// static text
 	
 	var fixWidth = 180;	// static width
 	var fixTop1 = 39;	// static 생성위치 top
 	var fixTop2 = 26;	// static height size
 	var fixTop3 = 0;	// 2 depth menu top
	
 	var fixLeft = 0;	// static fixed left 	
 	var max = 6;		// 최대 매뉴 생성 width
 	var maxsum = max;
 	var arrCnt = Math.ceil(colCnt/max);	// 2 depth menu max count
 	var arrHeight = new Array(); 		// 2depth menu top size count
 	var top;			// static top
 	var lev = 0;		// 2depth menu height level
 	var levCnt = 0;		// level count
 	
 	var widCnt = 0;		// 가로 배치 보정 값
 	var leftCnt = 0;	// 가로 배치 보정 값
 	
 	var last = 0;		// 마지막 번째줄 보정값
 	var levGap = 10;	// level 과 level 공간

 	for(var i = 0; i < colCnt; i++)
 	{
		for(var j = 0; j < rowCnt; j++)
		{
			sta++;
			staId = "Static0"+sta;
			colId = "column0"+i;
			staText = nDs.getColumn(j, colId);
			
			if(this.gfn_isNull( staText ))	// text 값이 없으면 3depth 메뉴가 없음.
			{
				j = rowCnt;
			}
			else
			{
				if(i < maxsum)	// 6개씩 비교 하여 3depth 하위에 2depth 를 left=0 부터 다시 생성
				{
					widCnt = maxsum - i;
					leftCnt = max - widCnt;
										
					fixLeft = fixWidth * leftCnt;	// static left cacl
					
					//첫번째 마지막 위치보정
					if(leftCnt == 0)
					{
						fixLeft = fixLeft+1;
					}
					else if(leftCnt == (max-1))
					{
						fixLeft = fixLeft-1;
					}
					
					top = fixTop1 + (fixTop2*j) + fixTop3 + last;	// static top cacl
					
					//trace("leftCnt: " + leftCnt + " j: " + j);
					
					if(lev == j || lev < j)	// 3depth 최대 생성 값 저장
					{
						lev = j;
						arrHeight[levCnt] = lev;
					}
 				}
 				else
 				{
					//trace(arrHeight[levCnt]);
					levCnt++;
					if(arrCnt == (levCnt+1))	//마지막 보정값
					{
						if(2 == arrCnt)
						{
							last = -fixTop2;
						}
						else if(levCnt < arrCnt)
						{							
							last = levGap;
						}

					}
					else
					{
						fixTop1 = fixTop2;
					}
					
					var sum = 0;
					
					for(var k = 0; k < arrHeight.length; k++)
						sum = sum + arrHeight[k] + 1;
					
					fixTop3 = fixTop1 +(sum * fixTop2);
					top = fixTop1 + (fixTop2*j) + fixTop3 + last;
					
					fixLeft = 1;
					lev = 0;
					widCnt = 0;
					
 					maxsum = maxsum + max;	//6개씩 나누기 위해 더함
 					
 					if(arrHeight.length < arrCnt)	// height 값 보정
						arrHeight[levCnt] = 0;

 				}
 				
				objStatic = new Static(staId, "absolute", fixLeft, top, fixWidth, 26, null, null);
				objPdv.addChild(staId, objStatic);
				//this.Div00.addChild(staId, objStatic);
				
				if(j == 0)	// 2depth style
				{
					objStatic.lev = 2;
					objStatic.set_cssclass("sta_TF_Menu_level02");
					objStatic.style.set_font("10 notokr-regular, dotum" );	 
				}
				else	//3depth style
				{
					objStatic.lev = 3;
					objStatic.set_cssclass("sta_TF_Menu_level03");
				}
				
				objStatic.set_text(staText);
				objStatic.addEventHandler("onclick", function(obj, e)
													{
														var nDs = obj.parent.menudataset;
														var lev = obj.lev;
														
														var nRow 		= nDs.findRow( "MENU_NM", obj.text );	
														var MENU_CD 	= nDs.getColumn( nRow, "MENU_CD" );
														var MENU_NM 	= nDs.getColumn( nRow, "MENU_NM" );
														var ENG_MENU_NM = nDs.getColumn( nRow, "ENG_MENU_NM" );
														var PGM_ID 		= nDs.getColumn( nRow, "PGM_ID" );
														var UP_CD 		= nDs.getColumn( nRow, "UP_CD" );
														
														this.gfn_menuOpen(MENU_CD, MENU_NM, ENG_MENU_NM, PGM_ID, UP_CD, lev);
														
														objStatic.parent.closePopup();
													},
											this);
				objStatic.show();
			}
		}
 	}
 	
 	return arrHeight;	// popupsize 보정을 위해 return
}]]></Script>
